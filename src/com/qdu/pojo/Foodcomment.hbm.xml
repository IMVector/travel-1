<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.qdu.pojo.Foodcomment" table="foodcomment" schema="bishe">
        <id name="fCommentId" column="FCommentID"/>
        <property name="fCommentContent" column="FCommentContent"/>
        <property name="fCommentScore" column="FCommentScore"/>
        <property name="fCommentTime" column="FCommentTime"/>
    </class>
</hibernate-mapping>
<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.qdu.pojo.Scene" table="scene" schema="bishe">
        <id name="sceneId" column="SceneID"/>
        <property name="sceneName" column="SceneName"/>
        <property name="sceneImage" column="SceneImage"/>
        <property name="sceneScore" column="SceneScore"/>
    </class>
</hibernate-mapping>
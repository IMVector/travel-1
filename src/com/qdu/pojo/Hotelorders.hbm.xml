<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.qdu.pojo.Hotelorders" table="hotelorders" schema="bishe">
        <id name="hotelOrderId" column="HotelOrderID"/>
        <property name="hotelName" column="HotelName"/>
        <property name="hotelPrice" column="HotelPrice"/>
        <property name="orderDay" column="OrderDay"/>
        <property name="contact" column="Contact"/>
        <property name="orderName" column="OrderName"/>
        <property name="orderPeople" column="OrderPeople"/>
    </class>
</hibernate-mapping>
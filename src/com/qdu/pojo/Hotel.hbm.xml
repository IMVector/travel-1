<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.qdu.pojo.Hotel" table="hotel" schema="bishe">
        <id name="hotelId" column="HotelID"/>
        <property name="hotelName" column="HotelName"/>
        <property name="hotelPrice" column="HotelPrice"/>
        <property name="hotelLevel" column="HotelLevel"/>
        <property name="hotelScore" column="HotelScore"/>
        <property name="hotelImage" column="HotelImage"/>
    </class>
</hibernate-mapping>
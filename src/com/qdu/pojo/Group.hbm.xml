<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.qdu.pojo.Group" table="group" schema="bishe">
        <id name="gid" column="GID"/>
        <property name="gTime" column="GTime"/>
        <property name="gPrice" column="GPrice"/>
        <property name="gQuatity" column="GQuatity"/>
        <property name="gEndTime" column="GEndTime"/>
        <property name="gContent" column="GContent"/>
    </class>
</hibernate-mapping>
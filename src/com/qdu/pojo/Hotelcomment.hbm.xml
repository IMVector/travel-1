<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.qdu.pojo.Hotelcomment" table="hotelcomment" schema="bishe">
        <id name="hCommentId" column="HCommentID"/>
        <property name="hCommentContent" column="HCommentContent"/>
        <property name="hCommentScore" column="HCommentScore"/>
        <property name="hCommentTime" column="HCommentTime"/>
    </class>
</hibernate-mapping>
<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.qdu.pojo.Foodinfo" table="foodinfo" schema="bishe">
        <id name="fid" column="FID"/>
        <property name="fName" column="FName"/>
        <property name="fScore" column="FScore"/>
        <property name="fImage" column="FImage"/>
        <property name="fPrice" column="FPrice"/>
    </class>
</hibernate-mapping>
<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.qdu.pojo.Routeorders" table="routeorders" schema="bishe">
        <id name="routeOrderId" column="RouteOrderID"/>
        <property name="routePrice" column="RoutePrice"/>
        <property name="contact" column="Contact"/>
        <property name="routeOrderName" column="RouteOrderName"/>
        <property name="routeOrderPeople" column="RouteOrderPeople"/>
    </class>
</hibernate-mapping>